#' Sum up the differences in the genotype matrices in two genotypes list objects.
#'
#' @param genos1 A genotypes list object.
#' @param genos2 Another genotypes list object
#'
#' @return In summary of differences in the genotype matrices in two genotypes list objects.
#'
#' @examples reportGenoChanges(preImpuation,postImputation)

#' @export
reportGenoChanges <- function(genos1, genos2) {

  cat(paste("The absolute number of genotypes in", deparse(substitute(genos1)),"is:"))
  t1 <- table(genos1$ABHmatrix)
  print(t1)
  cat(paste("\n"))
  cat(paste("The absolute number of genotypes in", deparse(substitute(genos2)),"is:"))
  t2 <- table(genos2$ABHmatrix)
  print(t2)

  cat(paste("\n"))
  cat(paste("or in percentage\n"))
  cat(paste("\n"))

  cat(paste("The relative number of genotypes in", deparse(substitute(genos1)),"is:\n"))

  df1 <- data.frame("A" = round(100 / sum(t1) * t1[1], 3),
                    "B" = round(100 / sum(t1) * t1[2], 3),
                    "H" = round(100 / sum(t1) * t1[3], 3),
                    "N" = round(100 / sum(t1) * t1[4], 3))
  print(df1)
  cat(paste("\n"))
  cat(paste("The relative number of genotypes in", deparse(substitute(genos2)),"is:\n"))

  df2 <- data.frame("A" = round(100 / sum(t2) * t2[1], 3),
                    "B" = round(100 / sum(t2) * t2[2], 3),
                    "H" = round(100 / sum(t2) * t2[3], 3),
                    "N" = round(100 / sum(t2) * t2[4], 3))
  print(df2)

  #paste("fall025")
  #paste("There are ",100 / sum(t1) * t1[4], "% Ns pre error correction")
  #t2 <- table(postImp_preCor$ABHmatrix)
  #paste("There are ",100 / sum(t2) * t2[4], "% Ns post error correction")

}
